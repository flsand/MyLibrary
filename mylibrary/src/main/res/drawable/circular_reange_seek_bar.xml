<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/orange" />
            <size
                android:width="20dp"
                android:height="20dp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/white" />
        </shape>
    </item>

</layer-list>

